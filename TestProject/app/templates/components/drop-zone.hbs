<div class="" style="display:inline-block;background:white;position:relative;z-index:26">

  <div style="margin: 0 0 0.2em 0"><p id="uploadWarning"></p></div>

<!--form method="post" enctype="multipart/form-data" action="." class=""-->
  <button id="uploadPics" style="cursor:pointer;background:#eee"
  type="submit" class="btn-std hide-all" {{action 'processQueue'}}>&nbsp;Ladda upp&nbsp;</button>

  <button id="removeAll" style="cursor:pointer;background:#eee"
  type="submit" class="btn-std hide-all" {{action 'removeAllFiles'}} title="...från den här panelen">&nbsp;Ta bort allt&nbsp;</button>

  <button id="removeDup" style="cursor:pointer;background:#eee"
  type="submit" class="btn-std hide-all" {{action 'removeDupFiles'}}>&nbsp;Ta bort dubletter&nbsp;</button>
<!--/form-->

</div>

<p id="dzinfo">LADDA UPP FOTOGRAFIER</p>
<p id="uploadFinished"></p>
<p id="dzclose" {{action 'closeThis'}}> &times; </p>

<!-- an empty <div> breaks the inline block sequence -->
<div></div>

{{yield}}
